{
  "name": "@sanity/document-internationalization",
  "version": "0.3.8",
  "keywords": [
    "sanity",
    "cms",
    "headless",
    "realtime",
    "content",
    "sanity-plugin",
    "i18n",
    "localization"
  ],
  "bugs": {
    "url": "https://github.com/sanity-io/document-internationalization/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sanity-io/document-internationalization"
  },
  "license": "MIT",
  "author": "Sanity.io <hello@sanity.io>",
  "files": [
    "lib/",
    "doc/",
    "config.dist.json",
    "sanity.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "npm run eslint -- .",
    "eslint": "eslint --ext=.js,.jsx,.mjs,.ts,.tsx --quiet",
    "lint:fix": "eslint . --quiet --fix",
    "build": "run-p build:*",
    "build:type": "tsc -p .",
    "build:badges": "rollup -c src/badges/rollup.config.js",
    "build:structure": "rollup -c src/structure/rollup.config.js",
    "build:language-select": "rollup -c src/language-select/rollup.config.js",
    "build:actions": "rollup -c src/actions/rollup.config.js",
    "build:constants": "rollup -c src/constants/rollup.config.js",
    "build:utils": "rollup -c src/utils/rollup.config.js",
    "build:validators": "rollup -c src/validators/rollup.config.js",
    "watch": "run-p \"build:* -- -w\"",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && npm run lint && npm run build",
    "postpublish": "pinst --enable"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint . --quiet --fix"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@cprecioso/country-flag-emoji": "^1.0.0",
    "html-react-parser": "^1.4.12",
    "just-omit": "^2.0.1",
    "just-safe-get": "^4.0.1",
    "just-split": "^3.0.1",
    "pinst": "^2.1.6",
    "react-twemoji": "^0.5.0",
    "regenerator-runtime": "^0.13.7",
    "twemoji": "^14.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
